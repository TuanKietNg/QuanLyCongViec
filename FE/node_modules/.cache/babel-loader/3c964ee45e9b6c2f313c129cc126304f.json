    v-model=\"model.action\"\r\n                        type=\"text\"\r\n                        class=\"form-control\"\r\n                        placeholder=\"Nhập action\"\r\n                    />\r\n                  </div>\r\n                  <div class=\"mb-3\">\r\n                    <label class=\"text-left\">Icon</label>\r\n                    <input\r\n                        id=\"icon\"\r\n                        v-model=\"model.icon\"\r\n                        type=\"text\"\r\n                        class=\"form-control\"\r\n                        placeholder=\"Nhập icon\"\r\n                    />\r\n                  </div>\r\n                  <div class=\"mb-3\">\r\n                    <label class=\"text-left\">Menu cha</label>\r\n                    <treeselect\r\n                                        v-on:select=\"addCoQuanToModel\"\r\n                                            :normalizer=\"normalizer\"\r\n                        :options=\"treeView\"\r\n                        :value=\"model.parentId\"\r\n                        :searchable=\"true\"\r\n                        :show-count=\"true\"\r\n                        :default-expand-level=\"1\"\r\n\r\n                    >\r\n                      <label slot=\"option-label\" slot-scope=\"{ node, shouldShowCount, count, labelClassName, countClassName }\" :class=\"labelClassName\">\r\n                        {{ node.label }}\r\n                        <span v-if=\"shouldShowCount\" :class=\"countClassName\">({{ count }})</span>\r\n                      </label>\r\n                    </treeselect>\r\n                  </div>\r\n                  <div class=\"mb-3\">\r\n                    <label class=\"text-left\">Thứ tự</label>\r\n                    <input\r\n                        id=\"sort\"\r\n                        v-model=\"model.level\"\r\n                        type=\"number\"\r\n                        class=\"form-control\"\r\n                        placeholder=\"Nhập thứ tự\"\r\n                    />\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"text-end pt-2 mt-3\">\r\n                <b-button v-if=\"model.id\" type=\"button\" variant=\"warning\" class=\"ms-1\"\r\n                          v-on:click=\"handleResetForm\"\r\n                > Đặt lại\r\n                </b-button>\r\n                <b-button v-if=\"model.id\" type=\"button\" variant=\"danger\" class=\"ms-1\"\r\n                v-on:click=\"handleShowDeleteModal(model.id)\"\r\n                > Xóa\r\n                </b-button>\r\n                <b-button type=\"submit\" variant=\"success\" class=\"ms-1 cs-btn-primary px-3\"\r\n                          v-if=\"$can('save-menu')\"\r\n                >Lưu\r\n                </b-button>\r\n              </div>\r\n            </form>\r\n            <b-modal\r\n                v-model=\"showDeleteModal\"\r\n                centered\r\n                title=\"Xóa dữ liệu\"\r\n                title-class=\"font-18\"\r\n                no-close-on-backdrop\r\n            >\r\n              <p>\r\n                Dữ liệu xóa sẽ không được phục hồi !\r\n              </p>\r\n              <template #modal-footer>\r\n                <button v-b-modal.modal-close_visit\r\n                        class=\"btn btn-outline-info m-1\"\r\n                        v-on:click=\"showDeleteModal = false\">\r\n                  Đóng\r\n                </button>\r\n                <button v-b-modal.modal-close_visit\r\n                        class=\"btn btn-danger btn m-1\"\r\n                        v-on:click=\"handleDelete\">\r\n                  Xóa\r\n                </button>\r\n              </template>\r\n            </b-modal>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </Layout>\r\n</template>\r\n"],"mappings":";;;;;;AACA,OAAAA,MAAA;AACA,OAAAC,UAAA;AACA,SAAAC,QAAA;AACA,OAAAC,SAAA;AACA,SAAAC,WAAA;AACA,SAAAC,SAAA;AACA,OAAAC,UAAA;AAEA